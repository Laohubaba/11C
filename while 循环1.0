#include<stdio.h>
int main()
{
	int i = 1;
	while ( i <= 10 )
	{
		if (i == 5)
		continue; //后面是个死循环          //break 用于终止循环
		printf("%d ", i);
		i++;

//EOF - end of file -  文件结束标志
		//int ch = getchar();
		////printf("%c\n", ch);
		//putchar(ch);//输出一个字符


		int ch = 0;
		while ((ch = getchar())  != EOF)//!= 表示检查两个操作数的值是否相等
			
		{
			putchar(ch);
		}
		//ctrl+z -getchar 就读结束
	}

	return 0;
}
